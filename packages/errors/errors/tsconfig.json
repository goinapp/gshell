{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": true,
    "stripInternal": true,
    "outDir": "../gerror/dist"
  },
  "files": [
    "../gerror/src/index.ts"
  ],
  "exclude": [
    "../gerror/node_modules",
    "../gerror/dist"
  ],
  "include": [
    "../gerror/test/**/*.test.ts",
    "../gerror/src/**/*.ts"
  ]
}
